{
  "root": true,
  "extends": [
    "@galacticcouncil/eslint-config/index.js",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "no-restricted-imports": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [
    {
      "files": ["*.mjs"],
      "env": {
        "node": true,
        "es2022": true
      },
      "extends": [],
      "plugins": [],
      "parserOptions": { "sourceType": "module" }
    }
  ]
}
