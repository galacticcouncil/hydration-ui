{
  "name": "@galacticcouncil/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*/index.ts",
    "./fonts.css": "./src/assets/fonts/fonts.css"
  },
  "scripts": {
    "dev": "yarn theme && tsc --build && storybook dev --no-open -p 6006",
    "build": "tsc --build && storybook build",
    "preview": "http-server ./storybook-static -p 6007 -s",
    "lint": "yarn lint:eslint && yarn lint:ts",
    "lint:ts": "tsc --noEmit -p tsconfig.json",
    "lint:eslint": "eslint src",
    "theme": "node ./style-dictionary/build.mjs"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.12.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@radix-ui/react-switch": "^1.1.1",
    "lucide-react": "^0.459.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@galacticcouncil/eslint-config": "*",
    "@galacticcouncil/typescript-config": "*",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-themes": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@tokens-studio/sd-transforms": "^1.2.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint-plugin-storybook": "^0.10.1",
    "http-server": "^14.1.1",
    "storybook": "^8.3.6",
    "style-dictionary": "^4.1.4",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
